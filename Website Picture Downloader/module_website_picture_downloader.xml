<?xml version="1.0" encoding="UTF-8"?>
<project name="module_website_picture_downloader" default="compile.module.website_picture_downloader">
  <dirname property="module.website_picture_downloader.basedir" file="${ant.file.module_website_picture_downloader}"/>
  
  <property name="module.jdk.home.website_picture_downloader" value="${project.jdk.home}"/>
  <property name="module.jdk.bin.website_picture_downloader" value="${project.jdk.bin}"/>
  <property name="module.jdk.classpath.website_picture_downloader" value="${project.jdk.classpath}"/>
  
  <property name="compiler.args.website_picture_downloader" value="-encoding GBK -source 1.7 ${compiler.args}"/>
  
  <property name="website_picture_downloader.output.dir" value="${module.website_picture_downloader.basedir}/out/production/Website Picture Downloader"/>
  <property name="website_picture_downloader.testoutput.dir" value="${module.website_picture_downloader.basedir}/out/test/Website Picture Downloader"/>
  
  <path id="website_picture_downloader.module.bootclasspath">
    <!-- Paths to be included in compilation bootclasspath -->
  </path>
  
  <path id="website_picture_downloader.module.production.classpath">
    <path refid="${module.jdk.classpath.website_picture_downloader}"/>
  </path>
  
  <path id="website_picture_downloader.runtime.production.module.classpath">
    <pathelement location="${website_picture_downloader.output.dir}"/>
  </path>
  
  <path id="website_picture_downloader.module.classpath">
    <path refid="${module.jdk.classpath.website_picture_downloader}"/>
    <pathelement location="${website_picture_downloader.output.dir}"/>
  </path>
  
  <path id="website_picture_downloader.runtime.module.classpath">
    <pathelement location="${website_picture_downloader.testoutput.dir}"/>
    <pathelement location="${website_picture_downloader.output.dir}"/>
  </path>
  
  
  <patternset id="excluded.from.module.website_picture_downloader">
    <patternset refid="ignored.files"/>
  </patternset>
  
  <patternset id="excluded.from.compilation.website_picture_downloader">
    <patternset refid="excluded.from.module.website_picture_downloader"/>
  </patternset>
  
  <path id="website_picture_downloader.module.sourcepath">
    <dirset dir="${module.website_picture_downloader.basedir}">
      <include name="src"/>
    </dirset>
  </path>
  
  
  <target name="compile.module.website_picture_downloader" depends="compile.module.website_picture_downloader.production,compile.module.website_picture_downloader.tests" description="Compile module Website Picture Downloader"/>
  
  <target name="compile.module.website_picture_downloader.production" depends="register.custom.compilers" description="Compile module Website Picture Downloader; production classes">
    <mkdir dir="${website_picture_downloader.output.dir}"/>
    <javac2 destdir="${website_picture_downloader.output.dir}" debug="${compiler.debug}" nowarn="${compiler.generate.no.warnings}" memorymaximumsize="${compiler.max.memory}" fork="true" executable="${module.jdk.bin.website_picture_downloader}/javac">
      <compilerarg line="${compiler.args.website_picture_downloader}"/>
      <bootclasspath refid="website_picture_downloader.module.bootclasspath"/>
      <classpath refid="website_picture_downloader.module.production.classpath"/>
      <src refid="website_picture_downloader.module.sourcepath"/>
      <patternset refid="excluded.from.compilation.website_picture_downloader"/>
    </javac2>
    
    <copy todir="${website_picture_downloader.output.dir}">
      <fileset dir="${module.website_picture_downloader.basedir}/src">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
    </copy>
  </target>
  
  <target name="compile.module.website_picture_downloader.tests" depends="register.custom.compilers,compile.module.website_picture_downloader.production" description="compile module Website Picture Downloader; test classes" unless="skip.tests"/>
  
  <target name="clean.module.website_picture_downloader" description="cleanup module">
    <delete dir="${website_picture_downloader.output.dir}"/>
    <delete dir="${website_picture_downloader.testoutput.dir}"/>
  </target>
</project>